    "import time\n",
    "\n",
    "\n",
    "def retry_fetch_structured_query(\n",
    "    fetch_function, query_type, query, max_retries=3, delay=0\n",
    "):\n",
    "    for attempt in range(max_retries):\n",
    "        try:\n",
    "            result = fetch_function(query_type=query_type, query=query)\n",
    "            return result\n",
    "        except Exception as e:\n",
    "            print(f\"Attempt {attempt + 1} failed: {e}\")\n",
    "            if attempt < max_retries - 1:\n",
    "                time.sleep(delay)  # Wait before retrying\n",
    "    print(\"All attempts failed.\")\n",
    "    return None"
   ]

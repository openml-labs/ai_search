    "import re\n",
    "\n",
    "\n",
    "def parse_arguments(arg_str):\n",
    "    # Match all 'eq', 'lt', etc., statements and extract them\n",
    "    args = []\n",
    "    matches = re.findall(\n",
    "        r'(eq|ne|gt|gte|lt|lte|contain|like|in|nin)\\(\"([^\"]+)\",\\s*([^\\)]+)\\)', arg_str\n",
    "    )\n",
    "    for comparator, attribute, value in matches:\n",
    "        # Handle cases where value is a string or a number\n",
    "        if value.startswith('\"') and value.endswith('\"'):\n",
    "            value = value[1:-1]  # Remove surrounding quotes\n",
    "        else:\n",
    "            try:\n",
    "                value = int(value)  # Try converting to integer\n",
    "            except ValueError:\n",
    "                pass  # Leave as string if not an integer\n",
    "        args.append({\"comparator\": comparator, \"attribute\": attribute, \"value\": value})\n",
    "\n",
    "    return args\n",
    "\n",
    "\n",
    "def parse_expression(expr):\n",
    "    # expr = expr.strip().strip('()')\n",
    "\n",
    "    # Match operator 'and' or 'or' and recursively parse its arguments\n",
    "    match = re.match(r\"(and|or|not)\", expr)\n",
    "    if match:\n",
    "        operator = match.groups()[0]\n",
    "        return {\"operator\": operator, \"arguments\": parse_arguments(expr)}\n",
    "    else:\n",
    "        return parse_arguments(expr)[0]"
   ]

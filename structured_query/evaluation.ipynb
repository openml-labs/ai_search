    "def transform_response(response):\n",
    "    # Extract and sort arguments\n",
    "    arguments = response['arguments']\n",
    "    sorted_arguments = sorted(arguments, key=lambda x: x['attribute'])\n",
    "    \n",
    "    # Transform each argument\n",
    "    transformed_arguments = []\n",
    "    for arg in sorted_arguments:\n",
    "        if isinstance(arg['value'], str):\n",
    "            value_str = f'\"{arg[\"value\"]}\"'\n",
    "        else:\n",
    "            value_str = str(arg['value'])\n",
    "        transformed_arguments.append(f'eq(\"{arg[\"attribute\"]}\", {value_str})')\n",
    "    \n",
    "    # Combine into the final format\n",
    "    combined_arguments = ', '.join(transformed_arguments)\n",
    "    final_string = f'{response['operator']}({combined_arguments})'\n",
    "    \n",
    "    return final_string"
   ]

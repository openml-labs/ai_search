    "def normalize_filter(filter):\n",
    "    \"\"\"\n",
    "    Normalize a single filter for comparison.\n",
    "    \"\"\"\n",
    "    if isinstance(filter, dict):\n",
    "        return json.dumps(filter, sort_keys=True)\n",
    "    return filter\n",
    "\n",
    "\n",
    "def count_filter_match(gt_filter, generated_filters):\n",
    "    \"\"\"\n",
    "    Check if generated filters exactly match ground truth filters.\n",
    "    \"\"\"\n",
    "    if \"operator\" in gt_filter:\n",
    "        filters = gt_filter[\"arguments\"]\n",
    "    else:\n",
    "        filters = [gt_filter]\n",
    "    if \"operator\" in generated_filters:\n",
    "        generated_f = generated_filters[\"arguments\"]\n",
    "    else:\n",
    "        generated_f = [generated_filters]\n",
    "    return sum(1 for f in filters if f in generated_f)"
   ]
